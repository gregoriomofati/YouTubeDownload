LOOLOO_KIDS       = LooLooKids
LOOLOO_KIDS_DIR   = $(BASE_DIR)/$(LOOLOO_KIDS)
LOOLOO_KIDS_FILES = $(RAINBOW_CHICKS) $(EDUCATIONAL_SONGS) $(LULLABIES) \
                    $(NEW_SONGS) $(TOP_25_SONGS) $(ANIMAL_SONGS) \
                    $(THE_BEST_NURSERY_RHYMES_IN_ENGLISH)


$(LOOLOO_KIDS)_all: $(LOOLOO_KIDS)_dir $(patsubst %, $(LOOLOO_KIDS_DIR)/.%.fmt, $(LOOLOO_KIDS_FILES))

$(LOOLOO_KIDS)_dir:
	@mkdir --parents --verbose $(LOOLOO_KIDS_DIR)/

$(LOOLOO_KIDS)_clean:
	@rm --recursive --force --verbose $(LOOLOO_KIDS_DIR)/


TARGET_ALL   += $(LOOLOO_KIDS)_all
TARGET_CLEAN += $(LOOLOO_KIDS)_clean
DOT_PHONY    += $(LOOLOO_KIDS)_all $(LOOLOO_KIDS)_dir $(LOOLOO_KIDS)_clean
